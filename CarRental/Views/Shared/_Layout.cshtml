<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css"
          integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb" crossorigin="anonymous">
    <link rel='stylesheet prefetch' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css'>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <a class="navbar-brand" href="#">Navbar</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarColor02">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" id="master">Masters <span class="caret"></span></a>
                        <div class="dropdown-menu" aria-labelledby="masters">
                            @Html.ActionLink("Company", "CompanyList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Branch", "BranchList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Department", "DepartmentList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Employee", "EmployeeList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Party", "PartyList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Designation", "DesignationList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Car Model", "CarModelList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Car", "CarList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Guest", "GuestList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Scheme", "SchemeList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Rate Card", "RateCardList", "MasterData", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Expense Head", "ExpenseList", "MasterData", null, new { @class = "dropdown-item" })
                        </div>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" id="master">Booking <span class="caret"></span></a>
                        <div class="dropdown-menu" aria-labelledby="masters">
                            @Html.ActionLink("Booking", "BookingList", "Booking", null, new { @class = "dropdown-item" })
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">About</a>
                    </li>
                </ul>
                <form class="form-inline my-2 my-lg-0">
                    <input class="form-control mr-sm-2" type="text" placeholder="Search">
                    <button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
                </form>
            </div>
        </nav>
    </div>
    <div class="container body-content" style="margin-top:10px;">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @*<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
            integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>*@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js"
            integrity="sha384-vFJXuSJphROIrBnz7yo7oB41mKfc8JzQZiCq4NCceLEaO4IHwicKwpJf9c9IpFgh" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js"
            integrity="sha384-alpBpkh1PFOepccYVYDB4do5UnbKysX5WZXm3XxPqe5iKTfUKjNkCk9SaVuEZflJ" crossorigin="anonymous"></script>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script>
        $(document).ready(function () {
            $(".chosen-select-single").chosen();
        });
    </script>
</body>
</html>
